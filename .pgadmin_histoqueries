<histoqueries>
 <histoquery>SELECT id, name, value, clicks, invalid_clicks, conversions, invalid_conversions, 
       placement_stat_daily_id, impressions, unique_impressions, widget_clicks, 
       widget_clicks_unique, cost
  FROM up_tracking_placementparamsstatdaily limit 10;
</histoquery>
 <histoquery>SELECT id, name, value, clicks, invalid_clicks, conversions, invalid_conversions, 
       placement_stat_daily_id, impressions, unique_impressions, widget_clicks, 
       widget_clicks_unique, cost
  FROM up_tracking_placementparamsstatdaily order by id desc limit 10;
</histoquery>
 <histoquery>SELECT id, name, value, clicks, invalid_clicks, conversions, invalid_conversions, 
       placement_stat_daily_id, impressions, unique_impressions, widget_clicks, 
       widget_clicks_unique, cost
  FROM up_tracking_placementparamsstatdaily order by id desc limit 20;
</histoquery>
 <histoquery>SELECT id, impressions, clicks, invalid_clicks, conversions, invalid_conversions, 
       stat_date, action_id, cost, unique_impressions, widget_clicks, 
       widget_clicks_unique
  FROM up_tracking_actionstatdaily order by id limit 10;
</histoquery>
 <histoquery>SELECT id, impressions, clicks, invalid_clicks, conversions, invalid_conversions, 
       stat_date, action_id, placement_id, cost, unique_impressions, 
       widget_clicks, widget_clicks_unique
  FROM up_tracking_placementstatdaily limit 10;
</histoquery>
 <histoquery>SELECT id, impressions, clicks, invalid_clicks, conversions, invalid_conversions, 
       stat_date, action_id, placement_id, cost, unique_impressions, 
       widget_clicks, widget_clicks_unique
  FROM up_tracking_placementstatdaily limit 100;
</histoquery>
 <histoquery>SELECT id, impressions, clicks, invalid_clicks, conversions, invalid_conversions, 
       stat_date, action_id, placement_id, cost, unique_impressions, 
       widget_clicks, widget_clicks_unique
  FROM up_tracking_placementstatdaily limit 500;
</histoquery>
 <histoquery>SELECT id, impressions, clicks, invalid_clicks, conversions, invalid_conversions, 
       stat_date, action_id, placement_id, cost, unique_impressions, 
       widget_clicks, widget_clicks_unique
  FROM up_tracking_placementstatdaily where invalid_conversions != 0 limit 500;
</histoquery>
 <histoquery>SELECT id, name, value, clicks, invalid_clicks, conversions, invalid_conversions, 
       action_stat_daily_id, impressions, unique_impressions, widget_clicks, 
       widget_clicks_unique, cost
  FROM up_tracking_actionparamsstatdaily limit 10;
</histoquery>
 <histoquery>SELECT id, impressions, clicks, invalid_clicks, conversions, invalid_conversions, 
       hour, action_stat_daily_id, cost, unique_impressions, widget_clicks, 
       widget_clicks_unique
  FROM up_tracking_actionstathourly limit 100;
</histoquery>
